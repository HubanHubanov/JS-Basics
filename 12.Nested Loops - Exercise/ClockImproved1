function clock() {
  for (let hours = 0; hours <= 23; hours++) {
    for (let mins = 0; mins <= 59; mins++) {
      let buff = "";
      if (hours <= 9) {
        buff += "0" + hours;
      } else {
        buff += hours;
      }
      buff += ":";
      if (mins <= 9) {
        buff += "0" + mins;
      } else {
        buff += mins;
      }

      console.log(buff);
    }
  }
}

clock();
